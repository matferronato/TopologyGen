sudo ifconfig eth1 1.1.1.1/24 up
sudo ifconfig eth2 2.2.2.1/24 up
sudo ifconfig eth50 3.3.3.1/24 up
sudo ifconfig eth100 5.5.5.2/24 up
sudo sysctl -w net.ipv4.ip_forward=1
sudo ip route add 1.1.1.0/24 via 1.1.1.3 dev eth1
sudo ip route add 2.2.2.0/24 via 2.2.2.3 dev eth2
sudo ip route add 4.4.4.0/24 via 5.5.5.1 dev eth100
sudo ip route add 5.5.5.0/24 via 5.5.5.1 dev eth100
sudo ip route del 0/0
sudo ip route add 0/0 via 5.5.5.1 dev eth100 
sudo ip link add br0 type bridge
sudo ip link set br0 up
sudo ip link add name vxlan10 type vxlan id 10 dev eth100 group 239.1.1.1 dstport 4789
sudo ip link set dev eth1 master br0
sudo ip link set dev vxlan10 master br0
sudo ip link set vxlan10 up
